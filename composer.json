    "scripts": {
        "test": "./vendor/bin/phpunit __tests__",
        "check-style": "./vendor/bin/phpcs --standard=PSR12 src __tests__",
        "fix-style": "./vendor/bin/phpcbf --standard=PSR12 src __tests__"
    },
    "minimum-stability": "stable",
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "RuiF\\CsvJson\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RuiF\\CsvJsonTests\\": "__tests__"
        }
    }
