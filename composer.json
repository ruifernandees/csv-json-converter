{
    "name": "ruifernandees/csv-json-converter",
    "description": "A modern PHP CSV to JSON and JSON to CSV converter",
    "license": "MIT",
    "authors": [
        {
            "name": "ruifernandees",
            "email": "dev.ruifernandes@gmail.com"
        }
